{
  "name": "@beidou-internal/eslint-plugin-sdk",
  "version": "1.0.0",
  "description": "Official Beidou SDK eslint plugin",
  "repository": "git://github.com/getbeidou/beidou-javascript.git",
  "homepage": "https://github.com/getbeidou/beidou-javascript/tree/master/packages/eslint-plugin-sdk",
  "author": "Beidou",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "beidou"
  ],
  "engines": {
    "node": ">=6"
  },
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "mocha": "^6.2.0",
    "prettier": "1.19.0",
    "typescript": "3.7.5"
  },
  "scripts": {
    "link:yarn": "yarn link",
    "lint": "prettier --check \"{src,test}/**/*.js\"",
    "fix": "prettier --write \"{src,test}/**/*.js\"",
    "test": "mocha test --recursive",
    "pack": "npm pack"
  }
}
